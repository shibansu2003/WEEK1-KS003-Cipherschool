#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<"Enter tha value of a: "<<endl;
    cin>>a;
    cout<<"Enter the value of b :"<<endl;
    cin >>b;
    if (a>b){
        cout<<"the sum of the numbers are "<<a+b<<endl;
    }
    else if(a<b){
        cout<<"The subtraction of the number is "<<a-b<<endl;
    }
    else if(a==b){
        cout<<"Both the integers are equal"<<endl;
    }
    return 0;
}
